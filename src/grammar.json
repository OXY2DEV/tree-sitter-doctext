{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "doctext",
  "rules": {
    "documentation": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[\\n\\r]+"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_documentation_component"
              },
              {
                "type": "SYMBOL",
                "name": "newline_or_eof"
              }
            ]
          }
        ]
      }
    },
    "_documentation_component": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "task"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          },
          "named": true,
          "value": "line"
        },
        {
          "type": "SYMBOL",
          "name": "code_block"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "footer"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_property_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_content_comment"
        }
      ]
    },
    "_property_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_delimeter"
        },
        {
          "type": "FIELD",
          "name": "property",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "comment_property"
            },
            "named": true,
            "value": "string"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "comment_string"
                },
                "named": true,
                "value": "string"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_content_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment_delimeter"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "content",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "comment_property"
                },
                "named": true,
                "value": "string"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comment_delimeter": {
      "type": "STRING",
      "value": "#"
    },
    "comment_property": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[^:\\s][^:\\n\\r]+"
      }
    },
    "comment_string": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\S[^\\n\\r]+"
      }
    },
    "task": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[\\w \\t]+"
            },
            "named": true,
            "value": "string"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "task_scope"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "!"
                }
              },
              "named": true,
              "value": "breaking"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        },
        {
          "type": "FIELD",
          "name": "description",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "task_scope": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_scope_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_scope_name"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_scope_name": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "mention"
        },
        {
          "type": "SYMBOL",
          "name": "issue_reference"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[^\\s,\\)\\(@#]+"
          },
          "named": true,
          "value": "string"
        }
      ]
    },
    "footer": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "footer_type"
            },
            "named": true,
            "value": "string"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "STRING",
                "value": "#"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "description",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        }
      ]
    },
    "footer_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "BREAKING CHANGE"
        },
        {
          "type": "PATTERN",
          "value": "\\w[^:#\\s\\(\\!]*"
        }
      ]
    },
    "string": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_string_component"
        }
      }
    },
    "_string_component": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "code"
        },
        {
          "type": "SYMBOL",
          "name": "bold"
        },
        {
          "type": "SYMBOL",
          "name": "italic"
        },
        {
          "type": "SYMBOL",
          "name": "single_quote"
        },
        {
          "type": "SYMBOL",
          "name": "double_quote"
        },
        {
          "type": "SYMBOL",
          "name": "issue_reference"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "punctuation"
        },
        {
          "type": "SYMBOL",
          "name": "mention"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "SYMBOL",
          "name": "autolink"
        }
      ]
    },
    "word": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-Z_-]+"
      }
    },
    "url": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "http:\\/\\/\\S+"
          },
          {
            "type": "PATTERN",
            "value": "https:\\/\\/\\S+"
          },
          {
            "type": "PATTERN",
            "value": "www\\.\\S+"
          }
        ]
      }
    },
    "issue_reference": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PATTERN",
                  "value": "\\d+"
                }
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[a-zA-Z0-9_-]+"
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "/"
                }
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PATTERN",
                  "value": "[a-zA-Z0-9_-]+"
                }
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "STRING",
                  "value": "#"
                }
              },
              {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PATTERN",
                  "value": "\\d+"
                }
              }
            ]
          }
        ]
      }
    },
    "code": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "`"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\`"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\n\\r`]"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "`"
          }
        ]
      }
    },
    "italic": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "*"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\*"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\n\\r*]"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "*"
          }
        ]
      }
    },
    "bold": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "**"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\*"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\n\\r*]"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "**"
          }
        ]
      }
    },
    "autolink": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "<[^>]+>"
      }
    },
    "single_quote": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "'"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\'"
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\n\\r']"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "'"
          }
        ]
      }
    },
    "double_quote": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\\\""
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\n\\r\"]"
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[+-]?\\.\\d+"
          },
          {
            "type": "PATTERN",
            "value": "[+-]?\\d+"
          },
          {
            "type": "PATTERN",
            "value": "[+-]?\\d+\\.\\d+"
          }
        ]
      }
    },
    "punctuation": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\p{P}",
        "flags": "u"
      }
    },
    "mention": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "@"
          },
          {
            "type": "PATTERN",
            "value": "\\w+"
          }
        ]
      }
    },
    "code_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "```"
          },
          "named": true,
          "value": "start_delimeter"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "language",
              "content": {
                "type": "ALIAS",
                "content": {
                  "type": "PATTERN",
                  "value": "[\\w_]+"
                },
                "named": true,
                "value": "string"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "SYMBOL",
            "name": "code_block_content"
          }
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "```"
          },
          "named": true,
          "value": "end_delimeter"
        }
      ]
    },
    "code_block_content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^`]+"
            },
            {
              "type": "PATTERN",
              "value": "`\\{1,2}[^`]+"
            }
          ]
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "newline_or_eof"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}