{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "doctext",
  "rules": {
    "documentation": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "header"
          },
          {
            "type": "SYMBOL",
            "name": "paragraph"
          }
        ]
      }
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "header_type"
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "SYMBOL",
            "name": "header_content"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\n"
            },
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "STRING",
              "value": ";"
            }
          ]
        }
      ]
    },
    "header_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\w+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "header_mention"
              },
              "named": true,
              "value": "mention"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "header_mention": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "PATTERN",
          "value": "[^)]+"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "header_content": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_inline"
      }
    },
    "paragraph": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_inline"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\n\\n?"
        }
      ]
    },
    "_inline": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "code"
        },
        {
          "type": "SYMBOL",
          "name": "italic"
        },
        {
          "type": "SYMBOL",
          "name": "bold"
        },
        {
          "type": "SYMBOL",
          "name": "issue_reference"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "punctuation"
        },
        {
          "type": "SYMBOL",
          "name": "mention"
        }
      ]
    },
    "issue_reference": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "PATTERN",
                "value": "\\d+"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "[a-zA-Z0-9_-]+"
              },
              {
                "type": "STRING",
                "value": "/"
              },
              {
                "type": "PATTERN",
                "value": "[a-zA-Z0-9_-]+"
              },
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "PATTERN",
                "value": "\\d+"
              }
            ]
          }
        ]
      }
    },
    "url": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "http://"
                  },
                  {
                    "type": "STRING",
                    "value": "https://"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "www."
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9@:%_\\+.~#=-]{2,256}"
          },
          {
            "type": "STRING",
            "value": "."
          },
          {
            "type": "PATTERN",
            "value": "[a-z]{2,6}"
          },
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9@:%_\\+.~#?&//=-]+"
          }
        ]
      }
    },
    "code": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\\`"
              },
              {
                "type": "PATTERN",
                "value": "[^`]"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    },
    "italic": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\\*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "*"
        }
      ]
    },
    "bold": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "**"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\\*"
              },
              {
                "type": "PATTERN",
                "value": "[^*]"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "**"
        }
      ]
    },
    "word": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\w+"
      }
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\d.]+"
      }
    },
    "punctuation": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\"'`\\(\\)\\[\\]\\{\\}\\|!#$%&\\*\\+\\-.,/:;<=>?@\\^_~]"
      }
    },
    "mention": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "@"
          },
          {
            "type": "PATTERN",
            "value": "\\w+"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}